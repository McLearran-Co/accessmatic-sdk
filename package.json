{
  "name": "accessmatic-sdk",
  "version": "1.0.0",
  "description": "Government-resilient client SDK for automatic PDF accessibility processing",
  "main": "src/accessmatic.js",
  "scripts": {
    "build": "node build.js",
    "build:watch": "node build.js --watch",
    "dev": "node build.js --dev",
    "test": "node test/test.js",
    "deploy": "npm run build && node deploy.js",
    "serve": "python3 -m http.server 8000 || python -m SimpleHTTPServer 8000"
  },
  "keywords": [
    "accessibility",
    "pdf",
    "wcag",
    "government",
    "compliance",
    "sdk"
  ],
  "author": "AccessMatic",
  "license": "MIT",
  "homepage": "https://accessmatic.us",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/accessmatic-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/accessmatic-sdk/issues"
  },
  "devDependencies": {
    "terser": "^5.19.2",
    "chokidar": "^3.5.3"
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
